<%= form_for @booking do |f|%>
<%# byebug %>

<%= f.label "Booking Title" %>
<%= f.text_field :name %><br>

<% booker = Booker.find(@booking.booker_id) %>
<%# f.label "Booker" %>
<%# f.collection_select :booker_id, [booker], :id, :name %>
<%= f.hidden_field :booker_id, value: booker.id %>

<%= f.label "Choose a Listing" %>
<%= f.collection_select :listing_id, Listing.all, :id, :name %><br>

<%= f.submit "Create Booking" %>
<% end %>
<hr>
<%= button_to "Cancel", booker_path(@booking.booker_id), method: :get %>