<h1>Create a Listing!</h1>

<%= form_for @listing do |f| %>
  <%= f.hidden_field :user_id, value: @user.id %>

  <%= f.label :location %>
  <%= f.text_field :location %><br>

  <%= f.label :start_date %>
  <%= f.date_field :start_date %><br>

  <%= f.label :end_date %>
  <%= f.date_field :end_date %><br>

  <%= f.submit %>
<% end %>
